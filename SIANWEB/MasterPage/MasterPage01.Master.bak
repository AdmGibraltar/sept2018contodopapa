            // ------------------------------------------------------------------------------------
            // Las sig. funcines validan que se capture en una caja de texto: NUMERO,NUMERO-NUMERO,NUMERO ej. 4,6,7-45,8
            var ultimoSeparador = '';
            function RangoEnterosSeparacionGuionYComas_OnKeyPress(sender, eventArgs) {
                //debugger;
                var texto = sender.get_textBoxValue() + eventArgs.get_keyCharacter();
                var c = eventArgs.get_keyCode();
                var key = eventArgs.get_keyCharacter();
                //si es ENTER --> se cancela
                if (c && c == 13) {
                    eventArgs.set_cancel(true);
                }
                else {
                    if ((c < 48 || c > 57) && (c != 44 && c != 45)) {//si no es numero, guionMedio y Coma --> se Cancela
                        eventArgs.set_cancel(true);
                    }
                    else {
                        if ((c < 48 || c > 57) && texto.length == 1) {//si no es numero y solamente es el primer caracter --> se Cancela
                            eventArgs.set_cancel(true);
                        }
                        else {
                            if (texto.length > 1) {
                                var caracter = texto.charAt(texto.length - 2);
                                var digitos = '0123456789';
                                if (digitos.indexOf(key) != -1) {
                                    //debugger;
                                    if (!EsEnteroValido(texto)) {
                                        eventArgs.set_cancel(true);
                                    }
                                    return;
                                }
                                if (caracter == ',' || caracter == '-') {
                                    if (digitos.indexOf(key) == -1) {
                                        eventArgs.set_cancel(true);
                                        return;
                                    }
                                }
                                if (digitos.indexOf(caracter) != -1) {
                                    if (key != ',' && key != '-') {
                                        eventArgs.set_cancel(true);
                                        return;
                                    }
                                }
                                //debugger;
                                if (key == '-' && ObtenerUltimoSeparador(texto) == '-') {
                                    eventArgs.set_cancel(true);
                                    return;
                                }
                                if (key == ',' || key == '-') {
                                    ultimoSeparador = key;
                                }
                            }
                        }
                    }
                }
            }

            function EsEnteroValido(texto) {
                var digitos = '0123456789';
                if (texto.length >= 10) {
                    for (i = texto.length - 1; i >= texto.length - 10; i--) {
                        if (digitos.indexOf(texto.charAt(i)) == -1) {
                            return true
                        }
                    }
                }
                else {
                    return true
                }
                return false
            }

            function ObtenerUltimoSeparador(texto) {
                var digitos = '0123456789';
                for (i = texto.length - 2; i >= 0; i--) {
                    if (digitos.indexOf(texto.charAt(i)) == -1) {
                        return texto.charAt(i);
                    }
                }
                return '';
            }

            function RangoEnterosSeparacionGuionYComas_OnBlur(sender, eventArgs) {
                //debugger;
                var i = 0;
                var continuar = true;
                var alerta = null;
                var texto = sender.get_textBoxValue();
                if (texto.charAt(texto.length - 1) == ',' || texto.charAt(texto.length - 1) == '-') {
                    texto = texto.substring(0, texto.length - 1);
                    sender.set_value(texto)
                }
                var array = texto.split(',');

                for (i = 0; i < array.length; i++) {
                    var rango = array[i].split('-');
                    if (rango.length == 2) {
                        if (parseInt(rango[0]) > parseInt(rango[1])) {
                            alerta = radalert('El rango del campo no es correcto. El número a la izquierda del guión debe ser menor al número de la derecha', 330, 150, tituloMensajes);
                            break;
                        }
                    }
                }
                if (alerta != null) {
                    continuar = false;
                }
                return continuar;
            }

            function RangoEnterosSeparacionGuion_OnKeyPress(sender, eventArgs) {
                //debugger;
                var texto = sender.get_textBoxValue() + eventArgs.get_keyCharacter();
                if (/^\d{1,9}$/.test(texto)) {
                    return;
                }
                else {
                    if (/^\d{1,9}\-$/.test(texto)) {
                        return;
                    }
                    else {
                        if (!(/^\d{1,9}\-\d{1,9}$/.test(texto))) {
                            eventArgs.set_cancel(true);
                        }
                    }
                }
            }

            function RangoEnterosSeparacionGuion_OnBlur(sender, eventArgs) {
                //debugger;
